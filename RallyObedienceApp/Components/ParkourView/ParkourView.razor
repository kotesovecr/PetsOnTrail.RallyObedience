@inherits ParkourViewBase

@using SkiaSharp
@using SkiaSharp.Views.Blazor

@if (Parkour is null)
{
    <h4>Loading ...</h4>
}
else
{
    <h1>@Parkour.ID</h1>
    <h2>@Parkour.Author</h2>
    <p>@((MarkupString) Parkour.Description)</p>
    <h3>@Parkour.Category</h3>
}

<div class="parkour-container">
    <canvas></canvas>
</div>